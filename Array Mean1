import java.util.Scanner;
public class Arraymean
{
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int c=0;
        float d=0;
        int arr[] = new int[n];
        for(int i=0;i<n;i++){
            arr[i] = sc.nextInt();
        }
        for(int i=0;i<n;i++){
        c=c+arr[i];
        d=c/n;
        }
         System.out.println(d);
        
    }
}

import java.util.Scanner;
public class Assessment2
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        float d=(int)c;
        float e=d+a;
        float f=d/50;
        float g=e-f;
        System.out.printf("\n %1.2f ",d);
        System.out.printf("\n %1.2f ",e);
        System.out.printf("\n %1.2f ",f);
        System.out.printf("\n %1.2f ",g);
    }
}

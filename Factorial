import java.util.Scanner;
public class Factorial
{
    public static void main(String[] args)
    {
        //Write your code
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int f=1,a=1;
        while(f<n)
        {
            a=a+1;
            f=f*a;
        }
        if(f==n)
        System.out.println("Yes");
        else
        System.out.println("no");
    }
}

import java.util.Scanner;
public class Paper
{
    public static void main(String args[])
    {
        //write your code
        Scanner sc=new Scanner(System.in);
        int w=sc.nextInt();
         int x=sc.nextInt();
          int y=sc.nextInt();
          int z=w*x;
          int a=w*y;
          int b=z-a-100;
       
          System.out.println(b);
    }
}

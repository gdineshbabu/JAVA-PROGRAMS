import java.util.Scanner;
public class Perfect
{
    public static void main(String[] args)
    {
        //Write your code
        Scanner sc=new Scanner(System.in);
        int sum=0;
        int n=sc.nextInt();
        int i=1;
        while(i<=n/2)
        {
            if(n%i ==0)
            {
                sum=sum+i;
            }
         i++;
        }
        if(sum==n)
        System.out.println("Perfect Number");
        else
        System.out.println("Not a perfect number");
        
    }
}

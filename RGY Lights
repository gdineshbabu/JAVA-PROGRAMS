import java.util.Scanner;
public class Lights
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        float s=sc.nextFloat();
        float d=sc.nextFloat();
        float t=sc.nextFloat();
        float sp=s/d;
        if(sp>t)
        System.out.println("Yes");
        else
        System.out.println("no");
    }
}

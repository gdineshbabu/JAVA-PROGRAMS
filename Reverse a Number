import java.util.Scanner;
public class Reverse
{
    public static void main(String[] args)
    {
        //Write your code
        Scanner sc=new Scanner(System.in);
        int n,sum=0,digit,temp;
         n=sc.nextInt();
        temp=n;
        while(n>0)
        {
            digit=n%10;
            sum=sum*10+digit;
            n=n/10;
        }
        if(sum>0)
        System.out.println(sum);
        
    }
}

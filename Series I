import java.util.Scanner;
public class Series1
{
    public static void main(String[] args)
    {
        //Write your code
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int s;
        for(int i=1;i<=n;i++)
        {
            s=i*i;
            System.out.print(s+" ");
        }
    }
}

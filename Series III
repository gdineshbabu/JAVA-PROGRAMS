import java.util.Scanner;
public class Series3
{
    public static void main(String[] args)
    {
        //Write your code
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int i,m=3,s;
        for(i=1;i<=n;i++)
        {
            s=(int)Math.pow(3,i);
            System.out.printf("%d ",s);
        }
    }
}

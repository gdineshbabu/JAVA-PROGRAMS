import java.util.Scanner;
public class Series9
{
    public static void main(String[] args)
    {
        //Write your code
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        long a=2;
        System.out.print(a+" ");
        for(int i=1;i<=n-1;i++)
        {
            if(i<=n)
            {
            a=(a*a)-1;
            System.out.print(a+" ");
            a=a;
            }
           
            
            
           
        }
        
    }
}

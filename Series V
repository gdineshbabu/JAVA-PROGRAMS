import java.util.Scanner;
public class Series5
{
    public static void main(String[] args)
    {
        //Write your code
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int i;
        int s=11;
        
        for(i=1;i<=n;i++)
        {
           
          System.out.print((int)Math.pow(s,2));
          System.out.print(" ");
           s=s+4;
        }
    }
}

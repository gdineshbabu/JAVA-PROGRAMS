import java.util.Scanner;
public class Sum
{
    public static void main(String[] args)
    {
        //Write your code
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int sum=0,digit;
        while(n>0)
        {
            digit=n%10;
            sum=sum+digit;
            n=n/10;
           
        }
        if(sum==sum)
         System.out.println(sum);
    }
}

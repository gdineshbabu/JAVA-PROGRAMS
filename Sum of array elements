import java.util.Scanner;
public class Sumarray
{
    public static void main(String[] args)
    {
        //Write your code
        Scanner sc=new Scanner(System.in);
        int sum=0,i;
        int n=sc.nextInt();
        int a[]=new int[n];
        
        for( i=0;i<n;i++)
        {
             a[i]=sc.nextInt();
           
        }
         for( i=0;i<n;i++)
        {
          sum=sum+a[i];
           
        }
        System.out.println(sum);
    }
}

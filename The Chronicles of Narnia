import java.util.Scanner;
public class Narnia
{
    public static void main(String args[])
    {
        //write your code
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
       
        
           int  digit=n%10;
           int  a=n/10;
            int sum=a+digit;
            
        
        
        System.out.println(sum);
        
    }
}

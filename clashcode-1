import java.util.*;
import java.io.*;
import java.math.*;

/**
 * Auto-generated code below aims at helping you parse
 * the standard input according to the problem statement.
 **/
class Solution {

    public static void main(String args[]) {
        Scanner in = new Scanner(System.in);
        int N = in.nextInt();
        int c=0;
        for (int i = 0; i < N; i++) {
            int xi = in.nextInt();
            c=c+xi*xi;
        }
        System.out.print(c);
    }
}
